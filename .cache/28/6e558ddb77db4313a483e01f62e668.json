ent: center;\n    \n}\n\n.img__container {\n    width: 425px;\n    height: 425px;\n}\n\n\n// IMAGES //\n\n.image1 {\n    @include imgSize;\n    filter: saturate(1.25);\n}\n\n.image1-black, .image1-white {\n    @include imgSize;\n    opacity: 0;\n}\n\n\n// COLOR LAYERS //\n\n.color {\n    @include imgSize;\n    mix-blend-mode: hue;\n}\n\n\n// INPUTS //\n\n#hue__input, #saturate__input, #luminosity__input {\n    @include inputStyle;\n}\n\n\n// BARS //\n\n.hue__bar {\n    background-image: linear-gradient(to right, red, magenta, blue, cyan, green, yellow, red);\n    @include barStyle;\n}\n\n.saturate__bar {\n    background-image: linear-gradient(to right, gray, red);\n    @include barStyle\n}\n\n.luminosity__bar {\n    background-image: linear-gradient(to left, white, black);\n    @include barStyle;\n}"},"lineCount":null}},"error":null,"hash":"0441d1f7890eb794929fbc22ff3c8dc0","cacheData":{"env":{}}}