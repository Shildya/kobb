\n    -webkit-appearance: none;\n}\n\n@mixin barStyle {\n    border-radius: 60px;\n    border-style: solid;\n    border-width: thin;\n    width: 200px;\n}\n\n\n// CONTAINERS //\n\n.customization__container {\n    display: flex;\n    flex-direction: row;\n    flex-wrap: wrap;\n    justify-content: center;\n    \n}\n\n.img__container {\n    width: 425px;\n    height: 425px;\n}\n\n.custom-system__container {\n    display: table\n}\n\n\n// IMAGES //\n\n.image1 {\n    @include imgSize;\n    filter: saturate(1.25);\n}\n\n.image1-black, .image1-white {\n    @include imgSize;\n    opacity: 0;\n}\n\n\n// COLOR LAYERS //\n\n.color {\n    @include imgSize;\n    mix-blend-mode: hue;\n}\n\n\n// INPUTS //\n\n#hue__input, #saturate__input, #luminosity__input {\n    @include inputStyle;\n}\n\n\n// BARS //\n\n.hue__bar {\n    background-image: linear-gradient(to right, red, magenta, blue, cyan, green, yellow, red);\n    @include barStyle;\n}\n\n.saturate__bar {\n    background-image: linear-gradient(to right, gray, red);\n    @include barStyle\n}\n\n.luminosity__bar {\n    background-image: linear-gradient(to left, white, black);\n    @include barStyle;\n}"},"lineCount":null}},"error":null,"hash":"e10eaf5906a80cb287ad975f97dc0eac","cacheData":{"env":{}}}