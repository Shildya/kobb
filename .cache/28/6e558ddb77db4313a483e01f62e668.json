in imgSize {\n    position: absolute;\n    width: 100%;\n    height: 100%;\n    top: 0;\n}\n\n@mixin inputStyle {\n    width: 195px;\n    height: 10px;\n    background: transparent;\n    border-color: transparent;\n    -webkit-appearance: none;\n}\n\n@mixin barStyle {\n    border-radius: 60px;\n    border-style: solid;\n    border-width: thin;\n    width: 200px;\n}\n\n// CONTAINERS //\n\n.customization {\n    margin-top: 50px;\n    display: flex;\n    justify-content: center;\n\n    @media (max-width: 600px) {\n        display: block;\n        margin-top: 25px;\n    }\n}\n\n.customization__container {\n    display: flex;\n    flex-direction: row;\n    flex-wrap: wrap;\n    width: 100%;\n}\n\n.image__container {\n    width: 30vw;\n    height: 30vw;\n    max-width: 840px;\n    max-height: 840px;\n    min-width: 300px;\n    min-height: 300px;\n    position: relative;\n\n    @media (max-width: 600px) {\n        left: 50%;\n        transform: translateX(-50%);\n    }\n}\n\n.custom-system__container {\n    height: 30vw;\n    max-height: 840px;\n    min-height: 300px;\n    position: relative;\n    width: 197px;\n\n    @media (max-width: 600px) {\n        left: 50%;\n        transform: translateX(-50%);\n    }\n}\n\n.settings__container {\n    position: absolute;\n    margin-top: 10px;\n\n    @media (max-width: 600px) {\n        margin-top: 10px;\n    }\n}\n\n.part__title__container {\n    position: relative;\n    height: 20px;\n}\n\n.part__title {\n    position: absolute;\n    left: 50%;\n    top: 50%;\n    transform: translate(-50%, -50%);\n    font-family: AdiHaus;\n    font-weight: bold;\n    white-space: nowrap;\n}\n\n.arrow {\n    height: 20px;\n}\n\n.left-arrow {\n    position: absolute;\n    left: 0;\n    opacity: 0;\n}\n\n.right-arrow {\n    position: absolute;\n    right: 0;\n}\n\n.hsl__container {\n    position: relative;\n    margin-top: 20px;\n}\n\n// IMAGES //\n\n.image {\n    @include imgSize;\n    filter: saturate(1.25);\n}\n\n.black, .white {\n    @include imgSize;\n    opacity: 0;\n}\n\n// COLOR LAYERS //\n\n// INPUTS //\n\n#hue__input, #saturate__input, #luminosity__input {\n    @include inputStyle;\n}\n\n\n// BARS //\n\n.hue__container, .saturation__container, .luminosity__container {\n    position: relative;\n    margin-top: 15px;\n    text-align: center;\n}\n\n.hue__bar {\n    background-image: linear-gradient(to right, red, yellow, green, cyan, blue, magenta, red);\n    @include barStyle;\n}\n\n.saturate__bar {\n    background-image: linear-gradient(to right, gray, red);\n    @include barStyle;\n    fill: green;\n}\n\n.luminosity__bar {\n    background-image: linear-gradient(to left, white, black);\n    @include barStyle;\n}\n\n// TEXT //\n\n.arrow {\n    color: gray;\n    cursor: pointer;\n}\n\n.hsl__title {\n    margin-bottom: 5px;\n    font-family: AdiHaus;\n    font-style: italic;\n}\n\n.customization__title {\n    display: block;\n    color: black;\n    font-family: AdiHaus;\n    text-align: center;\n    font-weight: bold;\n    font-size: 30px;\n    margin-top: 100px;\n    text-decoration: underline;\n    text-decoration-color: yellowgreen;\n}\n\n.menu {\n    top: 50px;\n}\n\n.size__title {\n    margin-top: 30px;\n    margin-bottom: 5px;\n    font-family: AdiHaus;\n    font-style: italic;\n    left: 50%;\n    transform: translateX(-50%);\n    position: absolute;\n}\n\n.sizes__container {\n    left: 50%;\n    transform: translateX(-50%);\n    width: 235px;\n    margin-top: 55px;\n    position: absolute;\n    display: flex;\n    justify-content: space-between;\n}\n\n.size__button {\n    background-color: transparent;\n    outline: 1px solid gray;\n    color: gray;\n    height: 30px;\n    width: 30px;\n    border: none;\n    font-family: AdiHaus;\n    font-size: 12px;\n    font-weight: bold;\n    line-height: 2.5;\n    text-align: center;\n\n    &.selected {\n        outline: 1px solid black;\n        color: black;\n    }\n}\n\n\n\n.buy__button__container {\n    width: 145px;\n    margin-top: 100px;\n    position: absolute;\n    text-align: center;\n    left: 50%;\n    transform: translateX(-50%);\n}\n  \n.buy__button {\nfont-family: AdiHaus;\nfont-style: normal;\nfont-weight: bold;\nfont-size: 12px;\ncolor: #ffffff;\ntext-decoration: none;\nbackground: yellowgreen;\nborder-radius: 8px;\npadding: 8px 20px;\nborder: none;\n\n&:hover {\n    filter: brightness(80%);\n    }\n}"},"lineCount":null}},"error":null,"hash":"e4e7cd4f5277e3f7a9bdab5dd9fa3b43","cacheData":{"env":{}}}