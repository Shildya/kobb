},{"source":"..\\css\\homepage.scss","name":null,"original":{"line":300,"column":2},"generated":{"line":289,"column":2}},{"source":"..\\css\\homepage.scss","name":null,"original":{"line":301,"column":2},"generated":{"line":290,"column":2}}],"sources":{"..\\css\\homepage.scss":"// PRESENTATION //\r\n\r\n.product__section {\r\n  width: auto;\r\n  height: calc(100% - 48px);\r\n  position: relative;\r\n  overflow: hidden;\r\n  left: 50%;\r\n  transform: translateX(-50%);\r\n}\r\n\r\n.background {\r\n  width: 100%;\r\n  height: 100%;\r\n  object-fit: cover;\r\n  position: absolute;\r\n  left: 50%;\r\n  transform: translateX(-50%);\r\n}\r\n\r\n.dude {\r\n  position: absolute;\r\n  right: calc(20% - 10vh);\r\n  bottom: 0;\r\n  height: 80%;\r\n  filter: saturate(80%);\r\n}\r\n\r\n.product {\r\n  position: absolute;\r\n  right: calc(20% - 10vh);\r\n  bottom: 0;\r\n  height: 80%;\r\n\r\n  &.loaded {\r\n      animation: colorChange 8s ease 0s infinite;\r\n  }\r\n}\r\n\r\n@keyframes colorChange {\r\n  17% {\r\n      filter: hue-rotate(120deg); \r\n  };\r\n  33% {\r\n      filter: hue-rotate(120deg);\r\n  }\r\n  50% {\r\n      filter: hue-rotate(240deg);\r\n  }\r\n  67% {\r\n      filter: hue-rotate(240deg);\r\n  }\r\n  83% {\r\n      filter: hue-rotate(360deg);\r\n  }\r\n  100% {\r\n      filter: hue-rotate(360deg);\r\n  }\r\n}\r\n\r\n.kobb-vest {\r\n  position: absolute;\r\n  width: calc(25vw + 20vh);\r\n  left: 5vw;\r\n  bottom: 75%;\r\n}\r\n\r\n.slogan {\r\n  position: absolute;\r\n  padding: 15px 20px;\r\n  width: 40vw;\r\n  height: fit-content;\r\n  font-family: AdiHaus;\r\n  font-style: normal;\r\n  font-weight: bold;\r\n  font-size: 4.5vw;\r\n  color: white;\r\n  background-color: #000000;\r\n  left: 5%;\r\n  top: 30%;\r\n  text-justify: distribute;\r\n}\r\n\r\n.buttons__container {\r\n  display: flex;\r\n  position: absolute;\r\n  bottom: 7.5%;\r\n  left: 5%;\r\n\r\n  @media (max-width: 666px) and (min-height: 404px) {\r\n    display: block;\r\n    top: 65%;\r\n    left: 10vw;\r\n  }\r\n}\r\n\r\n.button {\r\n  font-family: AdiHaus;\r\n  font-style: normal;\r\n  font-weight: bold;\r\n  font-size: calc(2vw + 1vh);\r\n  box-shadow: 0px 4px 4px rgba(0, 0, 0, 0.25);\r\n  border-radius: 8px;\r\n  padding: 12px 20px;\r\n  text-decoration: none;\r\n  border: none;\r\n  text-align: center;\r\n\r\n  &.customize {\r\n    background-color: yellowgreen;\r\n    color: white;\r\n\r\n    @media (max-width: 666px) and (min-height: 404px) {\r\n      margin-top: 3vh;\r\n    }\r\n  }\r\n\r\n  &.learn-more {\r\n    background-color: white;\r\n    color: black;\r\n    margin-left: 20px;\r\n\r\n    @media (max-width: 666px) and (min-height: 404px) {\r\n      margin-left: 0px;\r\n      margin-top: 2vh;\r\n    }\r\n  }\r\n}\r\n\r\n// MODELS //\r\n\r\n.models__title {\r\n  font-family: AdiHaus;\r\n  font-style: normal;\r\n  font-weight: bold;\r\n  font-size: 4.5vw;\r\n  line-height: 76px;\r\n  text-align: center;\r\n  margin-left: auto;\r\n  margin-right: auto;\r\n  color: #000000;\r\n  margin-top: 5px;\r\n  transform: translateY(15px);\r\n}\r\n\r\n.models {\r\n  position: relative;\r\n  width: 100%;\r\n  display: flex;\r\n  justify-content: space-around;\r\n  flex-wrap: wrap;\r\n\r\n  &>* {\r\n    flex: 1 1 160px;\r\n    margin: 10px;\r\n  }\r\n\r\n  @media (max-width: 800px) {\r\n    display: block;\r\n    text-align: center;\r\n\r\n    &>* {\r\n      display: inline-block;\r\n    }\r\n  }\r\n\r\n}\r\n\r\n.model__container {\r\n  width: 300px;\r\n  height: 300px;\r\n  box-shadow: 0px 4px 20px rgba(0, 0, 0, 0.25);\r\n}\r\n\r\n.image__container {\r\n  position: relative;\r\n  height: 250px;\r\n  width: 100%;\r\n  left: 50%;\r\n  transform: translateX(-50%);\r\n}\r\n\r\n.product__image {\r\n  position: absolute;\r\n  height: 200px;\r\n  top: 50%;\r\n  left: 50%;\r\n  transform: translate(-50%, -50%);\r\n}\r\n\r\n.description__container {\r\n  position: relative;\r\n  height: 50px;\r\n  width: 100%;\r\n  background-color: black;\r\n}\r\n\r\n.text__container 